{
  "name": "@diamondlightsource/periodic-table",
  "version": "0.2.3",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "src/PeriodicTable.tsx",
  "module": "./dist/periodic-table.mjs",
  "types": "./dist/PeriodicTable.d.ts",
  "exports": {
    "./refactored-periodic-table": "./src/PeriodicTable.tsx"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-mf": "^2.0.1",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && npm run copy-package && npm run copy-readme",
    "copy-package": "cp package.dist.json ./dist/package.json",
    "copy-readme": "cp README.dist.md ./dist/README.md",
    "publish-package": "npm version patch --no-git-tag-version && cd dist && npm version patch && cp package.json ../package.dist.json && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.56.1",
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "vite-plugin-dts": "^3.7.3",
    "@vitejs/plugin-react": "4.2.1"
  }
}
